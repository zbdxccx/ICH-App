<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.contest.ichapp.mapper.UserInfoMapper">
    <resultMap id="BaseResultMap" type="com.contest.ichapp.pojo.domain.UserInfo">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="user_id" jdbcType="VARCHAR" property="userId"/>
        <result column="sign" jdbcType="VARCHAR" property="sign"/>
        <result column="collected_id" jdbcType="VARCHAR" property="collectedId"/>
        <result column="history_id" jdbcType="VARCHAR" property="historyId"/>
    </resultMap>
    <select id="selectToCount" resultType="java.lang.Integer">
        select count(*)
        from user_info
        where user_id = #{userId,jdbcType=INTEGER}
          and collected_id =
              #{collectedId,jdbcType=INTEGER}
            limit 1
    </select>

</mapper>